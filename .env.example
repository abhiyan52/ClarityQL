# Copy to .env and fill with real values. Do not commit .env.

# Database
DATABASE_URL=postgresql+psycopg://postgres:postgres@127.0.0.1:5432/clarityql

# LLM Configuration (Gemini is default)
LLM_PROVIDER=gemini
LLM_MODEL=gemini-2.0-flash
LLM_TEMPERATURE=0.0

# Provider API Keys (only one needed based on LLM_PROVIDER)
GOOGLE_API_KEY=           # For Gemini (default)
OPENAI_API_KEY=           # For OpenAI
ANTHROPIC_API_KEY=        # For Anthropic
